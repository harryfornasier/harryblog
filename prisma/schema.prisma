generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model posts {
  id         Int       @id @default(autoincrement())
  title      String    @db.VarChar
  content    String
  created_at DateTime? @default(now()) @db.Timestamptz(6)
}
